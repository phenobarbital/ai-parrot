include LICENSE
include README.md
include pyproject.toml
include setup.py
global-include *.typed

# Whitelist: only include parrot package
graft parrot

# Global excludes
global-exclude *.pyc
global-exclude __pycache__
global-exclude *.so
global-exclude *.o
global-exclude .git*
global-exclude .DS_Store
global-exclude .venv

prune settings
prune plugins/*
prune /static/*
prune */__pycache__